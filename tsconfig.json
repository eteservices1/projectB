{
  "compilerOptions": {
     "lib": [
        "es5",
        "es6",
        "es2019",
        "dom"
     ],
     "target": "es5",
     "module": "commonjs",
     "moduleResolution": "node",
     "rootDir": "./",
     "outDir": "./build",
     "resolveJsonModule": true,
     "emitDecoratorMetadata": true,
     "experimentalDecorators": true,
     "sourceMap": true,
     "esModuleInterop": true,
     "jsx": "react"
  }
}